{
  "title": "Golang源码解读",
  "description": "一本解读Golang源码的书籍",
  "author": "张凯",
  "language": "zh-CN",
  "root": ".",
  "links": {
    "sidebar": {
      "Go语言中文网": "http://studygolang.com",
      "张小凯的博客": "https://jasonkayzk.github.io/"
    }
  },
  "plugins": [
    "-lunr",
    "-search",
    "-livereload",
    "highlight",
    "search-plus@^0.0.11",
    "simple-page-toc@^0.1.1",
    "github@^2.0.0",
    "github-buttons@2.1.0",
    "edit-link@^2.0.2",
    "anchors@^0.7.1",
    "include-codeblock@^3.0.2",
    "emphasize@^1.1.0",
    "mermaid@^0.0.9",
    "tbfed-pagefooter@^0.0.1",
    "expandable-chapters-small@^0.1.7",
    "sectionx@^3.1.0",
    "donate@^1.0.2",
    "anchor-navigation-ex",
    "favicon@^0.0.2",
    "todo@^0.1.3",
    "pageview-count"
  ],
  "pluginsConfig": {
    "theme-default": {
      "showLevel": false
    },
    "github": {
      "url": "https://github.com/jasonkayzk/go2source"
    },
    "github-buttons": {
      "repo": "jasonkayzk/go2source",
      "types": [
        "star"
      ],
      "size": "small"
    },
    "include-codeblock": {
      "template": "ace",
      "unindent": true,
      "edit": true
    },
    "sharing": {
      "weibo": true,
      "facebook": true,
      "twitter": true,
      "google": false,
      "instapaper": false,
      "vk": false,
      "all": [
        "facebook",
        "google",
        "twitter",
        "weibo",
        "instapaper"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "Copyright © jasonkayzk.github.io 2020",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "donate": {
      "wechat": "https://jasonkay_image.imfast.io/images/wechat.jpg",
      "alipay": "https://jasonkay_image.imfast.io/images/alipay.jpg",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    },
    "simple-page-toc": {
      "maxDepth": 3,
      "skipFirstH1": true
    },
    "edit-link": {
      "base": "https://github.com/JasonkayZK/go2source/tree/git-book",
      "label": "编辑该页面"
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    },
    "sectionx": {
      "tag": "b"
    },
    "favicon": {
      "shortcut": "favicon.ico",
      "bookmark": "favicon.ico"
    }
  }
}